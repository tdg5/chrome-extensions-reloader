{
  "name": "Zensight Extensions Reloader",
  "manifest_version": 2,
  "version": "0.0.1",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "tabs",
    "management",
    "webRequest",
    "*://*.zsinternal.com/",
    "*://app.zensight.co/"
  ],
  "browser_action": {
    "default_icon": "icon19.png",
    "default_title": "Reload Zensight Extensions"
  },
  "description": "Automatically reload Zensight extensions when assume-user URLs are visited."
}
